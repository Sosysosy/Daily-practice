    <title>Document</title>
    <script>
        //无参数则返回当前时间
        var date=new Date(); //实例化
        console.log(date);  
        
        //参数写法：数字型 2022,9,28  或者  字符串型 '2022-9-28 17:6:10'
        var date1=new Date(2022,9,28);
        console.log(date1);
        
        var date2=new Date('2022-9-28 17:6:10');
        console.log(date2);
    </script>
</head>
