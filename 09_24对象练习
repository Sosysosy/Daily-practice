    <script>
        //1.创建一个电脑对象，要有颜色、重量、品牌、型号，可以看电影、听音乐、打游戏和敲代码。
        var Computer = {
            color: '黑色',
            weight: '300kg',
            type: 'ASUS',
            model: '2022',
            mode: function () {
                '看电影';
                '听音乐';
                '打游戏';
                '敲代码';
            }
        }
        for(var k in Computer){
            console.log(Computer[k]);
        }
    </script>
