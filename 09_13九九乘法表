<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>
    <script>
        var num = '';
        for (var i = 1; i <= 9; i++) {
            for (var j = 1; j <= i; j++) {
                //    num += i+'*'+j+'='+i*j+' ';
                var k = 0;
                k = i * j;
                num += j + '*' + i + '=' + k + ' ';//输出格式，注意字符串的拼接 
            }
            num += '\n';
        }
        console.log(num);
    </script>
</head>

