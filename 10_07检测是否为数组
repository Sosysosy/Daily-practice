    <script>
        //检测是否为数组
        //1. instanceof 运算符 可以用来检测是否为数组
        var arr=[];
        var obj={};
        console.log(arr instanceof Array);  
        console.log(obj instanceof Array);

        //2. Array.isArray(参数)
        console.log(Array.isArray(arr));  //是数组返回ture
        console.log(Array.isArray(obj));  //不是数组返回false    
    </script>
