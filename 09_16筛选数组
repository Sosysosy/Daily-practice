    <script>
        //将数组[2,0,6,1,77,0,52,0,25,7]中大于等于10的元素选出来，放入新数组。
        var arr = [2, 0, 6, 1, 77, 0, 52, 0, 25, 7];
        var newarr = [];
        //法1
        // var j = 0;
        // for (var i = 0; i < arr.length; i++) {
        //     if (arr[i] >= 10) {
        //         newarr[j] = arr[i];
        //         j++;
        //     }
        // }
        //法2
        for (var i = 0; i < arr.length; i++) {
            if (arr[i] >= 10) {
                newarr[newarr.length] = arr[i];
            }
        }
            console.log(newarr);
